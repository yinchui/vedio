{
  "name": "ai-video-editor-frontend",
  "version": "0.1.0",
  "description": "AI智能剪辑助手 - 前端应用",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build:renderer": "tsc && vite build",
    "build:electron": "esbuild src/main.ts src/preload.ts --bundle --platform=node --target=node18 --format=cjs --outdir=dist-electron",
    "build:electron:watch": "npm run build:electron -- --watch",
    "build": "npm run build:renderer && npm run build:electron",
    "electron:dev": "concurrently -k \"npm run dev\" \"npm run build:electron:watch\" \"wait-on http://127.0.0.1:5173 file:dist-electron/main.js && electron dist-electron/main.js\"",
    "electron:build": "npm run build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "antd": "^5.10.0",
    "axios": "^1.6.8",
    "electron": "^27.3.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "socket.io-client": "^4.7.5",
    "video.js": "^8.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron-builder": "^24.13.3",
    "esbuild": "^0.20.2",
    "typescript": "^5.4.5",
    "vite": "^4.5.3",
    "wait-on": "^8.0.3"
  }
}
